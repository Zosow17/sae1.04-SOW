<style>
    .documents-container {
        display: flex;
        flex-wrap: wrap;
        gap: 20px;
        justify-content: center;
    }

    .petite-fenetre {
        width: 500px;
        height: 350px;
        border: 1px solid #ccc;
        position: relative;
        text-align: center;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: space-between;
    }

    .petite-fenetre iframe {
        width: 100%;
        height: 80%;
    }

    .apercu-agrandi {
        display: none;
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 80vw;
        height: 80vh;
        background: white;
        justify-content: center;
        align-items: center;
        z-index: 1000;
        padding: 20px;
        box-shadow: 0px 0px 15px rgba(178, 169, 169, 0.89);
        border-radius: 10px;
        border: 2px solid #000;
    }

    .apercu-agrandi iframe {
        width: 100%;
        height: 100%;
        border-radius: 5px;
    }

    .fermer {
        position: absolute;
        top: 50px;
        right: 50px;
        background: red;
        color: white;
        border: none;
        padding: 8px 12px;
        cursor: pointer;
        font-size: 16px;
        border-radius: 5px;
    }

    
    table {
        width: 100%;
        border-collapse: collapse;
        background-color: white;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }

    thead {
        background-color: #e95420;
        color: white;
    }

    th, td {
        padding: 16px;
        border: 1px solid #ddd;
        text-align: left;
        vertical-align: top;
    }

    tbody tr:nth-child(even) {
        background-color: #f2f2f2;
    }

    th {
        font-size: 16px;
    }

    td {
        font-size: 15px;
    }
</style>
